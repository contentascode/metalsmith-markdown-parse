{
  "name": "metalsmith-markdown-parse",
  "description": "A metalsmith plugin to parse markdown into file metadata",
  "repository": "git://github.com/contentascode/metalsmith-markdown-parse.git",
  "version": "0.1.14",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/*.js",
    "build": "babel --source-maps -d lib src",
    "build:watch": "nodemon --watch src --exec npm run build",
    "test": "npm -s run lint && npm -s run build &&  mocha --compilers js:@babel/register",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/helper-module-imports": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "assert-dir-equal": "^1.0.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "babel-plugin-stack-trace-sourcemap": "^1.0.2",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.1",
    "jstransformer-pug": "^0.3.0",
    "metalsmith": "^2.3.0",
    "metalsmith-debug": "^1.2.0",
    "metalsmith-layouts": "^2.3.0",
    "metalsmith-paths": "^3.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "source-map-support": "^0.4.15"
  },
  "dependencies": {
    "@masala/parser": "^0.7.0",
    "async": "^3.2.0",
    "commonmark": "^0.29.1",
    "debug": "^3.1.0",
    "html2jade": "^0.8.6",
    "lodash": "^4.17.15",
    "markdown-it": "^8.4.2",
    "marked": "^0.8.0",
    "minimatch": "^3.0.5",
    "pug": "^2.0.3"
  }
}
